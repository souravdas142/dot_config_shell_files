! fonts
!#define font1         Droid Sans Mono
!#define font2         Inconsolata Nerd Font Mono
!#define font3         DejaVu Sans
!#define font4         Noto Sans Mono CJK JP
#define font1         Hack-Regular
#define font2         Hack-Bold
#define font3         DejaVu Sans
#define font4         Noto Sans Mono CJK JP
#define font5         Hack-RegularOblique

URxvt*font:           xft:font1:pixelsize=14, xft:font2:pixelsize=11:minspace=false, xft:font3:pixelsize=11,xft:font4:pixelsize=11,xft:symbola
URxvt*boldFont:       xft:font2:bold:pixelsize=14, xft:font2:pixelsize=14:weight=bold:minspace=False,xft:font3:pixelsize=14:weight=bold,xft:font4:pixelsize=14:weight=bold,xft:symbola
URxvt*italicFont:     xft:font5:italic:pixelsize=14, xft:font2:pixelsize=14:minspace=False,xft:font3:pixelsize=14,xft:font4:pixelsize=14,xft:symbola
URxvt*boldItalicFont: xft:font5:bold:italic:pixelsize=16:weight=bold, xft:font2:pixelsize=14:weight=bold:minspace=False, xft:font3:pixelsize=14:weight=bold, xft:font4:pixelsize=14:weight=bold,xft:symbola


! do not jump to bottom when new output appears,
!        but do so on key press
URxvt*scrollTtyOutput: false
URxvt*scrollTtyKeypress: true

Xcursor.size: 9W
URxvt.urgentOnBell: true
URxvt.scrollBar: false
URxvt.matcher.button: 1
URxvt.perl-ext-common: default,matcher,clipboard,url-select,keyboard-select,ibeamcursor,clipboard-paste-on-right-click,selection-to-clipboard
xvt.url-select.launcher:  /usr/bin/firefox
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.keysym.M-c:       perl:clipboard:copy
URxvt.keysym.M-v:       perl:clipboard:paste
URxvt.keysym.M-C-v:     perl:clipboard:paste_escaped
URxvt.url-launcher: /usr/bin/firefox
URxvt.underlineURLs: true
URxvt.urlButton: 1
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.internalBorder:	10
URxvt*letterSpace:				0
URxvt.lineSpace:				0
URxvt*internalBorder:   0
URxvt*externalBorder:   0
URxvt*scrollBar_right:  false

! tabbed
URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 16
URxvt.tabbed.tab-fg:    15
URxvt.tabbed.tab-bg:    4
URxvt*buffered:         false 
